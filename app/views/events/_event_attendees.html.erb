<% if event.attendees.present? %>  
  <h3> <%= event.expired? ? "People who attended this event:": "People attending this event are:" %> </h3>
  <% event.attendees.each do |attendee| %>
    <p><%= attendee.name %> <span class="label label-<%= attendee.male? ? 'blue' : 'pink' %>"><i class="fa fa-<%= attendee.gender %> fa-2x" aria-hidden="true"></i></span></p>

  <% end %>
<% end %>

<%= content_for :head do %>
  <%= stylesheet_link_tag "events" %>
<% end %>
